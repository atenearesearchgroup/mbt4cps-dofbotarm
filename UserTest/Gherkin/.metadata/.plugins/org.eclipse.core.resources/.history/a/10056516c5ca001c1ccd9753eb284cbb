grammar org.xtext.example.mydsl.MyDsl with org.eclipse.xtext.common.Terminals

generate myDsl "http://www.xtext.org/example/mydsl/MyDsl"

Domainmodel :
    (elements+=Type)*;
  
Type:
    Operation;
  

    
Operation:
	RotateMultiple | RotateSingle
;

RotateMultiple:
	'RotateMultiple(' + INT + ',' + INT + ')'
;

RotateSingle:
	'RotateSingle(' + INT +')'
;