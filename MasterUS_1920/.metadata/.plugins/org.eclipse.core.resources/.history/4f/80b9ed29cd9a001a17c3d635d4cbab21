grammar lsi.us.es.mis.xtext.exercise.MyDomainModel with org.eclipse.xtext.common.Terminals

generate myDomainModel "http://www.us.lsi/es/mis/xtext/exercise/MyDomainModel"

Model :
	(types+=Type)+
;

Type :
	Class | DataType
;
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	

	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
DataType :
	'datatype' name=ID;
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
Class :
	(abstract?='abstract')? 'class' name=ID '{'
	(features+=Feature)*
	'}';
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
Feature :
	'attr' name=ID ':' type=[DataType];